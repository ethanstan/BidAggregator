{
  "name": "bidaggregator",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev:nollup": "run-p routify nollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "routify -b && rollup -c && routify export && workbox injectManifest",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "build:docker": "npm run build && ./scripts/docker/docker-build.sh",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css"
  },
  "devDependencies": {
    "@pageql/pageql-auth": "^1.0.1",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "nollup": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.26.5",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "^0.0.31",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-svelte-hot": "^0.11.0-0",
    "rollup-plugin-terser": "^6.1.0",
    "spassr": "^1.1.2",
    "tailwindcss": "^1.7.5"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@pageql/pageql-store-table": "^1.0.4",
    "@rollup/plugin-replace": "^2.3.3",
    "@sveltech/routify": "^1.9.9",
    "@tailwindcss/ui": "^0.5.0",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "graphql": "^15.3.0",
    "isomorphic-ws": "^4.0.1",
    "nhost-js-sdk": "^2.1.5",
    "postcss-import": "^12.0.1",
    "prosemirror-svelte": "^0.2.3",
    "sse.js": "^0.4.5",
    "subscriptions-transport-ws": "^0.9.18",
    "svelte": "^3.23.2",
    "svelte-apollo": "^0.4.0",
    "svelte-file-dropzone": "0.0.10",
    "svelte-preprocess": "^4.1.1",
    "svelte-select": "^3.11.1",
    "workbox-cli": "^5.1.3"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true,
    "routifyDir": ".routify"
  }
}
